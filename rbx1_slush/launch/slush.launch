<launch>
	<node name="overview_camera" pkg="cv_camera" type="cv_camera_node">
			<param name="image_width" value="640"/>
			<param name="image_height" value="480"/>
			<param name="frame_id" value="camera_link"/>
	</node>

	<include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch">
		<param name="port" value="9090"/>
	</include>

	 
    <node name="joint_states" pkg="rbx1_slush" type="joint_states_publisher.py"/>
    <node name="move_robot" pkg="rbx1_slush" type="joint_command_subscriber.py" output="screen"/>
</launch>